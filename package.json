{
  "name": "declare-test",
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/lib/index.js",
      "default": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  },
  "typings": "dist/lib/index",
  "description": "Compile-time type assertions for TypeScript.",
  "version": "0.1.0",
  "main": "dist/lib/index.js",
  "repository": "github:gregros/declare-test",
  "scripts": {
    "build": "tsc -b .",
    "clean": "shx rm -rf dist",
    "rebuild": "run-s clean build",
    "prepare": "husky install",
    "watch": "tsc -b . --watch",
    "lint:fix": "prettier --write src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,ctsx,mtsx,html,htm,yaml,yml,json,scss,css} && eslint --fix src/**/*.{ts,mts,cts,js,mjs,cjs} ",
    "lint:check": "prettier --check src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,ctsx,mtsx,html,htm,yaml,yml,json,scss,css} && eslint src/**/*.{ts,mts,cts,js,mjs,cjs}",
    "test": "run-s build && jest"
  },
  "keywords": [
    "typescript",
    "types",
    "testing",
    "assert",
    "expect",
    "type",
    "typing"
  ],
  "author": "GregRos",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,mts,cts,js,mjs,cjs}": "node_modules/.bin/eslint --cache --fix",
    "src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,ctsx,mtsx,html,htm,yaml,yml,json,scss,css}": "node_modules/.bin/prettier --write"
  },
  "packageManager": "yarn@4.1.1",
  "devDependencies": {
    "@gregros/eslint-config": "^0.6.0",
    "@swc/core": "^1.4.11",
    "@swc/jest": "^0.2.36",
    "@types/eslint": "^8",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.14",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}
